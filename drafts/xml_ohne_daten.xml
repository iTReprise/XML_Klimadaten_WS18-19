<?xml version="1.0" encoding="UTF-8"?>
<?xml-stylesheet type="text/xsl" href="http://www.imn.htwk-leipzig.de/~futku/climateData.xsl"?>
<!DOCTYPE climateData SYSTEM "http://www.imn.htwk-leipzig.de/~futku/climateData.dtd">
<climateData
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:noNamespaceSchemaLocation="http://www.imn.htwk-leipzig.de/~futku/climateData.xsd"
  locationID="94082">
  <longitude unit="decimal_degrees"></longitude>
  <latitude unit="decimal_degrees"></latitude>

  <entry date="yyyy.mm.dd" time="hh:mm" standard="UTC">
    <temperature>
      <curTemp unit="C" xsi:nil="false"></curTemp>
      <avgTemp unit="C" xsi:nil="false"></avgTemp>
      <maxTemp unit="C" xsi:nil="false"></maxTemp>
      <minTemp unit="C" xsi:nil="false"></minTemp>
    </temperature>

    <surface>
      <avgSurTemp unit="C" xsi:nil="false"></avgSurTemp>
      <maxSurTemp unit="C" xsi:nil="false"></maxSurTemp>
      <minSurTemp unit="C" xsi:nil="false"></minSurTemp>
    </surface>
    
    <solar>
      <avgSolRad unit="W/m²" xsi:nil="false"></avgSolRad>
      <maxSolRad unit="W/m²" xsi:nil="false"></maxSolRad>
      <minSolRad unit="W/m²" xsi:nil="false"></minSolRad>
    </solar>

    <humidity>
      <avgRelHum unit="%" xsi:nil="true"></avgRelHum>
    </humidity>
  </entry>

</climateData>